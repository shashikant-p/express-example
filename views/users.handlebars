<div class="container" style="margin-top: 80px;">
    {{> options}}
    <div>
        <table class="table table-striped">
            <tr class="thead-dark">
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
            {{#each userList}}
            <tr>
                <td>{{this.email}}</td>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>