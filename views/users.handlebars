<div class="container" style="margin-top: 80px;">
    <div class="container mt-4 mb-4">
        <div class="btn-group" role="group" aria-label="Basic example">
            <a href="#" type="button" class="btn btn-primary">Users</a>
            <a href="#" type="button" class="btn btn-secondary">Products</a>
            <a href="#" type="button" class="btn btn-secondary">Orders</a>
        </div>
    </div>

    <div>
        <table class="table table-striped">
            <tr class="thead-dark">
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
            {{#each userList}}
            <tr>
                <td>{{this.email}}</td>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>